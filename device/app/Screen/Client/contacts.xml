<?xml version="1.0" encoding="utf-8" ?>
<c:Screen xmlns:c="BitMobile.Controls" xmlns:s="BitMobile.ValueStack" StyleSheet="Client\contacts.css">

<c:SwipeHorizontalLayout Id="swipe_layout" Index="1">

  
  <c:VerticalLayout>

    <c:DockLayout CssClass="top_navigator">
         <c:HorizontalLayout CssClass="btn_back" OnEvent="Back" OnClickAction="$DoBackAndClean()">
   <c:Image/>
        <c:TextView Text="#back#"/>
       </c:HorizontalLayout>
      <c:Button Id="btnForward" Text="" CssClass="btn_forward"/>
      <c:HorizontalLayout>
        <c:TextView Text="Контакты" />
      </c:HorizontalLayout>
    </c:DockLayout>

    
		

<!--<s:Push Id="event" Value="$GetEventDetails($param1)"></s:Push>-->
<s:Push Id="exTasks" Value="$GetExecutedContact()"></s:Push>  
	
	    
   

	<!-- Begin all visits screen -->
			<c:ScrollView>
			<s:Iterator Id="eTask" Value="$exTasks">

			<c:SwipeHorizontalLayout Id="swipe_layout2" CssClass="swipe_layout">
					<c:DockLayout CssClass="grid">
						<c:Image />
						<c:VerticalLayout>
							<c:TextView Text="$eTask.FIO" CssClass="description_row"></c:TextView>
							<c:TextView Text="$eTask.Tel" CssClass="main_row"></c:TextView>
						</c:VerticalLayout>						
					</c:DockLayout>
					
				</c:SwipeHorizontalLayout>
				

			</s:Iterator>
				
	
	      </c:ScrollView>
	      	
	      


	<!-- End Body screen   -->
    
  </c:VerticalLayout>

</c:SwipeHorizontalLayout>

</c:Screen>